<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SDCard">
        <item name="cornerFamilyBottomLeft">cut</item>
        <item name="cornerSizeBottomLeft">8dp</item>
    </style>


    <style name="LoginEditTextCorner">
        <item name="cornerSize">4dp</item>
    </style>

    <style name="HomeCircleView">

        <item name="android:layout_width">64dp</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:background">@drawable/ellipse_1</item>

    </style>

    <style name="NoRightCorner">
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
    </style>


    <style name="CustomFontTextView" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/rambla</item>
    </style>

    <style name="Title" parent="CustomFontTextView">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="CornerOfButton">
        <item name="cornerSize">15dp</item>
    </style>

    <style name="CornerofImageView">
        <item name="cornerSize">35dp</item>
    </style>

    <style name="NavegationItems" parent="android:Widget.TextView">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="CalendarStyle" parent="android:Widget.Material.DatePicker" />

    <style name="ContentRowFieldLabel">
        <item name="android:textColor">@color/primaryDarkColor</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>